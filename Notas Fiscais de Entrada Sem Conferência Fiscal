SELECT PCNFENT.CODFILIAL,
       PCNFENT.DTENT,
       PCNFENT.DTEMISSAO,
       PCNFENT.NUMTRANSENT,
       PCNFENT.NUMNOTA,
       PCNFENT.ESPECIE,
       PCNFENT.CODFORNEC,
       PCFORNEC.FORNECEDOR,
       PCNFENT.VLTOTAL
  FROM PCNFENT, PCFORNEC
 WHERE PCNFENT.CONFERIDO IS NULL
   AND PCNFENT.CODFORNEC = PCFORNEC.CODFORNEC
   AND PCNFENT.CODFILIAL IN (&CODFILIAL)
   AND PCNFENT.DTENT BETWEEN TRUNC(SYSDATE) - 50 AND TRUNC(SYSDATE);
